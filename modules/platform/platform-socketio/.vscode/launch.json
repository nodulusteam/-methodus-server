    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "console": "internalConsole",
            "name": "T | Run jest",
            "type": "node",
            "runtimeExecutable": "yarn",
            "runtimeArgs": ["run", "--inspect-brk", "jest"],
            "stopOnEntry": false,
            "outputCapture": "std",
            "args": ["--detectOpenHandles", "--verbose"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "env": {}
        },
        {
            "request": "launch",
            "console": "internalConsole",
            "name": "T | Run test",
            "type": "node",
            "program": "${workspaceRoot}/dist/tests/socket.test.js",
            "stopOnEntry": false,
            "outputCapture": "std",
            "args": ["--verbose"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "env": {}
        }
    ]
}
