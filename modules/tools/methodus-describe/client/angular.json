{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "../public",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "lazyModules": [
              "src/modules/describe/describe.module",
              "src/modules/editor/editor.module"
            ],
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/ngx-monaco-editor/assets/monaco/",
                "output": "./assets/monaco/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/pxt-blockly/media/",
                "output": "./assets/blockly/media/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "src/sass/styles.scss",
              "src/assets/icons/style.css"
            ],
            "scripts": [
              "node_modules/pxt-blockly/blockly_compressed.js",
              "node_modules/pxt-blockly/blocks_compressed.js",
              "node_modules/pxt-blockly/msg/js/en.js",
              "src/assets/blockly/javascript_compressed.js",
              "src/assets/blockly/patch.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/pxt-blockly/blockly_compressed.js",
              "node_modules/pxt-blockly/blocks_compressed.js",
              "node_modules/pxt-blockly/msg/js/en.js",
              "src/assets/blockly/javascript_compressed.js",
              "src/assets/blockly/patch.js"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "src/sass/styles.scss",
              "src/assets/icons/style.css"
            ],
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/pxt-blockly/media/",
                "output": "./assets/blockly/media/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/ngx-monaco-editor/assets/monaco/",
                "output": "./assets/monaco/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "app:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "app",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    },
    "analytics": false
  }
}